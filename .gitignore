# Ignore files generated by Deno
./server/.deno
./server/.deno_lock
./server/.deno_dir

# Ignore compiled files and build artifacts for Deno
./server/*.js
./server/*.js.map
./server/*.tsbuildinfo

# Ignore node_modules folder for React
./client/node_modules/

# Ignore build artifacts for React
./client/build/

# Ignore nginx logs and temporary files
./nginx/logs/
./nginx/tmp/

# Ignore docker build artifacts
./docker/build/

# Ignore environment-specific files
./server/.env
./client/.env

# Ignore any other local or generated files
./server/.DS_Store
./client/.DS_Store
./nginx/.DS_Store
./docker/.DS_Store
./server/.idea/
./client/.idea/
./nginx/.idea/
./docker/.idea/