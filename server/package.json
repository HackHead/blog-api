{
  "name": "server",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts'  --exec 'npm run ts-node' src/App.ts",
    "ts-node": "ts-node",
    "build": "tsc -p ./dist",
    "start": "node dist/App.js",
    "prod": "pm2 start dist/App.js --name 'node-server'",
    "kill": "pm2 stop node-server && pm2 delete node-server",
    "reload": "yarn kill && yarn prod",
    "format": "prettier --write \"src/**/*.ts\"",
    "migrate": "NODE_ENV=development sequelize db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "wipe": "npx sequelize-cli db:seed:undo:all && npx sequelize-cli db:migrate:undo:all"
  },
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/register": "^7.22.5",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.3.1",
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@sindresorhus/slugify": "^2.2.1",
    "@types/validator": "^13.7.17",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "path": "^0.12.7",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "qs": "^6.11.2",
    "sequelize": "^6.32.1",
    "uuid": "^9.0.0",
    "validator": "^13.9.0",
    "winston": "^3.9.0"
  }
}
